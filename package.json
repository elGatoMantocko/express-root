{
  "name": "app",
  "version": "0.0.0",
  "description": "A website for wedding plans",
  "main": "src/main/server/index.js",
  "scripts": {
    "build": "npx gulp build",
    "start": "npx node src/main/server/index.js",
    "test": "npm test",
    "watch": "npx nodemon -w src/main/server/index.js -w src/main/assets/views -e \"js hbs\""
  },
  "author": "Elliott",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.1.0",
    "child_process": "^1.0.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-declare": "^0.3.0",
    "gulp-eslint": "^4.0.2",
    "gulp-less": "^4.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-postcss": "^7.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "gulp-wrap": "^0.13.0",

    "path": "^0.12.7",
    "popper.js": "^1.14.3",
    "tether": "^1.4.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "nodemon": "^1.17.3"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "node": "current"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "google",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "max-len": 1,
      "no-invalid-this": 0,
      "camelcase": 0
    }
  },
  "nodemonConfig": {
    "exec": "npm start"
  }
